#include<stdio.h>
int main()
{
    int i=0,j=0,k=0,n=0;
    for(i=1;i<=5;i++)
    {
        for(j=n;j>0;j--)
        {
            printf(" ");
        }
            for(k=5;k>=i;k--)
            {
                printf("%d",k);
            }
        printf("\n");
        n++;
    }
    return 0;
}
