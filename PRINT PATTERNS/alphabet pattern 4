#include<stdio.h>
int main()
{
    int i,j,k,n=4;
    for(i=1;i<=5;i++)
    {
        char a='A';
        for(j=n;j>0;j--)
        {
            printf(" ");
        }
            for(k=1;k<=i;k++)
            {
                printf("%c",a);
                a++;
            }
            n--;
            printf("\n");
        }
        return 0;
    }

